<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androiddeveloperbasics">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity
            android:name=".BooksApiActivity"
            android:exported="true" />
        <activity
            android:name=".AsyncTaskActivity"
            android:label="AsyncTask Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="Detail Activity"
            android:parentActivityName=".CardActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.CardActivity" />
        </activity>
        <activity
            android:name=".CardActivity"
            android:label="Card Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".ScorekeeperActivity"
            android:label="Scorekeeper Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".RecyclerActivity"
            android:label="Recycler Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".TabActivity"
            android:label="Tab Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".DialogActivity"
            android:label="Dialog Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".OrderActivity"
            android:label="Order Activity"
            android:parentActivityName=".CafeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.CafeActivity" />
        </activity>
        <activity
            android:name=".CafeActivity"
            android:label="Cafe Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".ImplicitIntentActivity"
            android:label="ImplicitIntent Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="Second Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
    </application>

</manifest>