<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androiddeveloperbasics">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".SettingsActivity"
            android:label="Settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".SharedPrefsActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>

        <service
            android:name=".NotificationJobService"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <activity
            android:name=".SchedulerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>

        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name=".AlarmActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".NotifyActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".BroadcastReceiverActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".BooksApiActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".AsyncTaskActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/app_name"
            android:parentActivityName=".CardActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.CardActivity" />
        </activity>
        <activity
            android:name=".CardActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".ScorekeeperActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".RecyclerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".TabActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".DialogActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".OrderActivity"
            android:label="@string/app_name"
            android:parentActivityName=".CafeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.CafeActivity" />
        </activity>
        <activity
            android:name=".CafeActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".ImplicitIntentActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
    </application>

</manifest>