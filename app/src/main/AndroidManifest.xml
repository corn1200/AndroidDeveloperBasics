<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androiddeveloperbasics">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".CardActivity"
            android:label="Card Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity"/>
        </activity>
        <activity
            android:name=".ScorekeeperActivity"
            android:label="Scorekeeper Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".RecyclerActivity"
            android:label="Recycler Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".TabActivity"
            android:label="Tab Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".DialogActivity"
            android:label="Dialog Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".OrderActivity"
            android:label="Order Activity"
            android:parentActivityName=".CafeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.CafeActivity" />
        </activity>
        <activity
            android:name=".CafeActivity"
            android:label="Cafe Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity
            android:name=".ImplicitIntentActivity"
            android:label="ImplicitIntent Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="Second Activity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androiddeveloperbasics.MainActivity" />
        </activity>
    </application>

</manifest>